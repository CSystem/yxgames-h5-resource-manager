        "outFile": "bin/resourcemanager/resourcemanager.js",
        "declaration": true,
        "noImplicitAny": false,
        "stripInternal": true,
        "allowUnreachableCode": true,
        "experimentalDecorators": true,
        "strictNullChecks": true,
        "sourceMap": false,
        "lib": [
