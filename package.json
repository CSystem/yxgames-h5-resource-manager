  "license": "ISC",
  "dependencies": {
    "crc32": "^0.2.2",
    "egret-node-utils": "0.0.7",
    "egret-typescript-ast": "0.0.11",
    "fs-extra-promise": "0.4.1",
    "map-stream": "0.0.6",
    "typescript": "2.1.6",
    "vinyl-fs": "^2.4.4",
    "watch": "^1.0.2",
    "htmlparser2": "^3.9.2",
    "vinyl": "^2.0.1",
    "mysql": "^2.13.0",
    "res": "^0.4.0",
    "jszip": "^3.1.4"
  },
  "devDependencies": {
    "@types/fs-extra-promise": "0.0.31",
    "@types/htmlparser2": "^3.7.29",
    "@types/node": "^7.0.8",
    "@types/vinyl": "^2.0.0",
